{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:laravel": "php artisan serve --host 127.0.0.1 --port 8000",
        "dev:react": "vite --host 127.0.0.1 --port 5173",
        "dev:full": "concurrently --kill-others-on-fail -n laravel,react,check -c blue,green,magenta \"npm:dev:laravel\" \"npm:dev:react\" \"node scripts/check-stack.js\""
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-react": "^4.3.4",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7"
    }
}
